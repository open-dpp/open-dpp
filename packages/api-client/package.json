{
  "name": "@open-dpp/api-client",
  "type": "module",
  "version": "2.2.1",
  "description": "A client for my API",
  "author": "open-dpp",
  "license": "ISC",
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc && rollup -c",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest",
    "check-ts": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "run-before-versions": "npm run lint && npm run build && npm run test",
    "prepatch": "npm run run-before-versions",
    "patch": "git fetch --tags && npm version patch && git push --follow-tags",
    "preminor": "npm run run-before-versions",
    "minor": "git fetch --tags && npm version minor && git push --follow-tags",
    "premajor": "npm run run-before-versions",
    "major": "git fetch --tags && npm version major && git push --follow-tags"
  },
  "dependencies": {
    "@open-dpp/dto": "workspace:*",
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@open-dpp/config-eslint": "workspace:*",
    "@open-dpp/testing": "workspace:*",
    "@rollup/plugin-typescript": "^12.3.0",
    "@swc/core": "^1.15.7",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.6",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.10.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "msw": "^2.12.4",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "rollup": "^4.54.0",
    "rollup-plugin-dts": "^6.3.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
